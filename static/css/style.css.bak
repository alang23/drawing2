/* flexslider */
.flexslider{margin:0 auto;position:relative;width:1000px;height:572px;overflow:hidden;zoom:1;}
.flex-viewport{max-height:2000px;-webkit-transition:all 1s ease;-moz-transition:all 1s ease;transition:all 1s ease;}
.flexslider .slides{zoom:1; margin-top:80px;}
.flex-direction-nav a{width:57px;height:63px;line-height:99em;overflow:hidden;margin:-60px 0 0;display:block;background:url(../images/part4_left.png) no-repeat;position:absolute;top:60%;z-index:10;cursor:pointer;opacity:1;filter:alpha(opacity=0);-webkit-transition:all .3s ease;}
.flex-direction-nav .flex-next{background:url(../images/part4_right.png) no-repeat; right:10px;}
.flex-direction-nav .flex-prev{left:10px;}
.flexslider:hover .flex-next{opacity:1;filter:alpha(opacity=100);}
.flexslider:hover .flex-prev{opacity:1;filter:alpha(opacity=100);}
.flexslider:hover .flex-next:hover, .flexslider:hover .flex-prev:hover{opacity:1;filter:alpha(opacity=100);}
.flex-control-nav{width:100%;position:absolute;bottom:10px;text-align:center;}
.flex-control-nav li{margin:0 5px;display:inline-block;zoom:1;*display:inline;}
.flex-control-paging li a{background:url(../images/dot.png) no-repeat 0 -16px;display:block;height:16px;overflow:hidden;text-indent:-99em;width:16px;cursor:pointer;}
.flex-control-paging li a.flex-active{background-position:0 0;}


