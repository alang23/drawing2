/* flash START */
.flash{float:left;width:100%;height:530px; position:relative; border-bottom:3px solid #22abeb;}

.ggLoading, .ggLoading2 {}
.gg { width: 100%; height: 530px; position: relative; z-index: 1; overflow: hidden; margin: 0 auto; background: #d3d3d3 url(loading.jpg) repeat-x; }
.ggLoading { position: absolute; left: 40%; top: 200px; width: 325px; text-align: center; height:56px; background-position: 0 -274px; background-repeat: no-repeat; line-height: 56px; color: #9c9c9c;}
.ggLoading2 { width: 330px; height: 56px; background-position: 213px -330px; background-repeat: no-repeat; }
.ggLoading em { font-weight: bold; }
.ggs { width: 200%; height: 530px; left: -50%; top: 0; position: absolute; }
.ggBox { width: 1920px; height: 530px; margin: 0 auto; }
.ggBox a { display: block; width: 1920px; height: 530px; position: absolute; z-index: 1; opacity: 0.1; }
.ggBox img { display: block; width: 1920px; height: 530px; }
.ggb { position: absolute; width: 100%; left: 0;bottom: 0; height: 20px; z-index: 4;}

.ggBtns { width:200px; height:10px;margin: 0 auto;margin-bottom:10px;}
.ggBtns a { float: left; display: block; width: 26px; height: 6px; text-align: center; position:relative;outline:none;cursor:pointer;background:url(../images/bannerbj2.jpg);margin-left:10px; display:inline;overflow:hidden;}
a.ggOn { background-image: url(../images/bannerbj1.jpg); }
